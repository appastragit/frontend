  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run launch-pod",
    "launch-pod": "npm run build-prod & npm run prod",
    "prod": "webpack-dev-server --config ./build/webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.js",
    "build-dev": "cross-env NODE_ENV=development node ./build/build.js",
    "build-prod": "node ./build/build.js",
    "postinstall": "cpy ./node_modules/framework7-icons/fonts/*.* ./src/fonts/",
    "lint": "cross-env eslint src --ext js,vue",
    "lint-fix": "cross-env eslint src --ext js,vue --fix",
    "before-push": "npm run lint"
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "dependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "axios": "^0.21.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.3.2",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "dom7": "^2.1.5",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
